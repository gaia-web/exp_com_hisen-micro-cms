{
  "importMap": "./imports.json",
  "tasks": {
    "dev": "DEV=1 PASSCODE=test deno run -A --unstable --watch app.ts",
    "dev:debugger": "DEV=1 PASSCODE=test deno run -A --unstable --watch --inspect-wait app.ts",
    "start": "deno run --allow-net --allow-env --unstable --watch app.ts",
    "build": "cd www && npm ci && npm run build"
  },
  "exclude": [
    "/www",
    "www_admin"
  ]
}
